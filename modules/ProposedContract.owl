    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#CauseOfContract -->

    <owl:Class rdf:about="&ProposedContract;CauseOfContract">
        <rdfs:label xml:lang="en">Cause of contract</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ProcurementsTopClasses;Function"/>
        <owl:disjointWith rdf:resource="&ProposedContract;EstimatedValueOfContract"/>
        <owl:disjointWith rdf:resource="&ProposedContract;ObjectOfContract"/>
        <owl:disjointWith rdf:resource="&ProposedContract;ProductOrServiceOrWork"/>
