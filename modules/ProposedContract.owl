<rdf:RDF xmlns="https://loted2.googlecode.com/svn/trunk/modules/ProposedContract#"
     xml:base="https://loted2.googlecode.com/svn/trunk/modules/ProposedContract"
     xmlns:v1="http://purl.org/goodrelations/v1#"
     xmlns:schema="http://schema.org/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ProcurementsTopClasses="https://loted2.googlecode.com/svn/trunk/modules/ProcurementsTopClasses#"
     xmlns:TenderBid="https://loted2.googlecode.com/svn/trunk/modules/TenderBid#"
     xmlns:ProcurementCompetitiveProcess="https://loted2.googlecode.com/svn/trunk/modules/ProcurementCompetitiveProcess#"
     xmlns:TenderDocuments="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:dc="http://purl.org/dc/terms/">

    <owl:Ontology rdf:about="&ProposedContract;">
        <owl:versionInfo>2.0</owl:versionInfo>
        <owl:versionIRI rdf:resource="https://loted2.googlecode.com/svn/trunk/modules/ProposedContract"/>
        <owl:imports rdf:resource="https://loted2.googlecode.com/svn/trunk/modules/TenderBid.owl"/>
		<!-- dc: metadata for documentation LODE -->
		<dc:creator>Isabella Distinto</dc:creator>
		<rdfs:label xml:lang="en">Proposed Contract</rdfs:label>
        <owl:versionInfo xml:lang="en">2.0</owl:versionInfo>
		<rdfs:comment xml:lang="en">The Proposed Contract module, which is part of the LOTED2 ontology, describes the categories of public contracts to be awarded through a procurement competitive process (e.g. a tender). Therefore, this module aims at represent the type of proposals of public contracts as they are described in the contract notices, not the public contract per se.</rdfs:comment>
        <dc:title rdf:datatype="&xsd;string">LOTED2: Proposed Contract Module</dc:title>
        <dc:contributor rdf:datatype="&xsd;string">Enrico Motta</dc:contributor>
        <dc:contributor rdf:datatype="&xsd;string">Mathieu d'Aquin</dc:contributor>
		<dc:description rdf:datatype="&xsd;string">The ontology was created by Isabella Distinto, with input from Mathieu d'Aquin and Enrico Motta.</dc:description>
        <dc:language rdf:datatype="&xsd;language">en</dc:language>
        <dc:date>2013-07-24</dc:date>
       <!-- dc: metadata end -->        
        
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>
    <owl:AnnotationProperty rdf:about="&rdfs;isDefinedBy"/>
    <owl:AnnotationProperty rdf:about="&ProposedContract;dataID"/>
    <owl:AnnotationProperty rdf:about="&ProposedContract;tedLabel"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#forWhichMayBeSubmitted -->

    <owl:ObjectProperty rdf:about="&ProposedContract;forWhichMayBeSubmitted">
        <rdfs:label xml:lang="en">for which may be submitted (1..*)</rdfs:label>
        <owl:inverseOf rdf:resource="&ProposedContract;mayBeSubmittedForAward"/>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedPublicProcurementContract"/>
        <rdfs:range rdf:resource="&TenderBid;TenderBid"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#involvesTheEstabilishmentOf -->

    <owl:ObjectProperty rdf:about="&ProposedContract;involvesTheEstabilishmentOf">
        <rdfs:label xml:lang="en">involves the estabilishment of (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">This property is used to express the relation between a LOTED2: ContractNotice and a LOTED2: FrameworkAgreement. In other words, through this property is inferred if a contract notice involves the estabilishment of a framework agreement that lays down terms and conditions for a series of public contracts awarded during a certain period of time and with one of several operators.</rdfs:comment>
        <rdfs:range rdf:resource="&ProposedContract;ProposedFrameworkAgreement"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#laysDownTermsAndConditionFor -->

    <owl:ObjectProperty rdf:about="&ProposedContract;laysDownTermsAndConditionFor">
        <rdfs:label xml:lang="en">lays down terms and conditions for (*)</rdfs:label>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedFrameworkAgreement"/>
        <rdfs:range rdf:resource="&ProposedContract;ProposedPublicContract"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#mayBeSubmittedForAward -->

    <owl:ObjectProperty rdf:about="&ProposedContract;mayBeSubmittedForAward">
        <rdfs:label xml:lang="en">may be submitted for award (1..*)</rdfs:label>
        <rdfs:range rdf:resource="&ProposedContract;ProposedPublicProcurementContract"/>
        <rdfs:domain rdf:resource="&TenderBid;TenderBid"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#subjectToTermsAndConditionsLaidDownWith -->

    <owl:ObjectProperty rdf:about="&ProposedContract;subjectToTermsAndConditionsLaidDownWith">
        <rdfs:label xml:lang="en">subject to terms and conditions laid down with (1..*)</rdfs:label>
        <rdfs:range rdf:resource="&ProposedContract;ProposedFrameworkAgreement"/>
        <owl:inverseOf rdf:resource="&ProposedContract;laysDownTermsAndConditionFor"/>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedPublicContract"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#atMainPlace -->

    <owl:ObjectProperty rdf:about="&ProposedContract;atMainPlace">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has place of performance of contract (1..1)</rdfs:label>
        <dataID>TW</dataID>
        <rdfs:comment xml:lang="en">This property links the LOTED2:ObjectOfContract with the LOTED2:Location
</rdfs:comment>
        <rdfs:range rdf:resource="&ProcurementsTopClasses;Location"/>
        <rdfs:domain rdf:resource="&ProposedContract;ObjectOfContract"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#concerns -->

    <owl:ObjectProperty rdf:about="&ProposedContract;concerns">
        <rdfs:label xml:lang="en">concerns (1..*)</rdfs:label>
        <rdfs:comment xml:lang="en">This property is meant to connect the LOTED2:ObjectOfContract to the LOTED2:Product to which it refers. 

For example: a LOTED2:ContractNotice has as LOTED2:ObjectOfContract the supply of laser printers and A4 paper for the Ministry of Health. 
This means that the Ministry of Health is searching for a Supply of laser print which includes the LOTED2: Product printers and printing paper. 

The products which are included in the object of the contract are identified through the CPV (Common Procurement Vocabulary) numenclature indicated in the tender documents (contract notice, periodic indicative notice, etc.).
The Common Procurement Vocabulary identifies more than 9400 products with a code composed of 8 numbers.

Referring to our example:

- Cpv code of Laser printers is 30232110
- Cpv code of printing paper is 30197630.

Compared to GoodRelations, this property aims to connect the gr: Offering (for example of a contract notice, that is defined through the class LOTED2: ObjectOfContract) with the gr: ProductOrService or gr: TypeAndQuantityNode (for multiple items) that specifies the type of gr: SomeItems and the amount of this good (data-type property).</rdfs:comment>
        <rdfs:domain rdf:resource="&ProposedContract;ObjectOfContract"/>
        <rdfs:range rdf:resource="&ProposedContract;ProductOrServiceOrWork"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#hasCauseOfContract -->

    <owl:ObjectProperty rdf:about="&ProposedContract;hasCauseOfContract">
        <rdfs:label xml:lang="en">has cause of contract (1..*)</rdfs:label>
        <rdfs:comment xml:lang="en">This property specifies the cause of the contract. 

For example: the individual LOTED2:Contract_for_supply_of_printers of type LOTED2:ProposedPublicSupplyContract is connected through LOTED2:hasCauseOfContract to the individual LOTED2:Buy of type LOTED2:CauseOfContract.</rdfs:comment>
        <rdfs:range rdf:resource="&ProposedContract;CauseOfContract"/>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedContract"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#hasEstimatedValueExcludingVAT -->

    <owl:ObjectProperty rdf:about="&ProposedContract;hasEstimatedValueExcludingVAT">
        <rdfs:label xml:lang="en">has estimated value excluding VAT (1..1)</rdfs:label>
        <rdfs:comment xml:lang="en">This links a LOTED2:ProposedPublicProcurementContract or LOTED2:ProposedFrameworkAgreement to a LOTED2:EstimatedValueOfContract.

The float value of the estimated value of contract is expressed through the data-type properties: 

LOTED2: hasCurrencyEstimatedvalue;
LOTED2: hasMinCurrencyEstimatedValue; 
LOTED2: hasMaxCurrencyEstimatedvalue.</rdfs:comment>
        <rdfs:range rdf:resource="&ProposedContract;EstimatedValueOfContract"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ProposedContract;ProposedFrameworkAgreement"/>
                    <rdf:Description rdf:about="&ProposedContract;ProposedPublicProcurementContract"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#hasMainPlaceOfDelivery -->

    <owl:ObjectProperty rdf:about="&ProposedContract;hasMainPlaceOfDelivery">
        <rdfs:label xml:lang="en">has main place of delivery (1..1)</rdfs:label>
        <rdfs:range rdf:resource="&ProcurementsTopClasses;Location"/>
        <rdfs:domain rdf:resource="&ProposedContract;Supply"/>
        <rdfs:subPropertyOf rdf:resource="&ProposedContract;atMainPlace"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#hasMainPlaceOfPerformance -->

    <owl:ObjectProperty rdf:about="&ProposedContract;hasMainPlaceOfPerformance">
        <rdfs:label xml:lang="en">has main place of performance (1..1)</rdfs:label>
        <rdfs:range rdf:resource="&ProcurementsTopClasses;Location"/>
        <rdfs:domain rdf:resource="&ProposedContract;Service"/>
        <rdfs:subPropertyOf rdf:resource="&ProposedContract;atMainPlace"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#hasMainPlaceOfWorks -->

    <owl:ObjectProperty rdf:about="&ProposedContract;hasMainPlaceOfWorks">
        <rdfs:label xml:lang="en">has main place of works (1..1)</rdfs:label>
        <rdfs:range rdf:resource="&ProcurementsTopClasses;Location"/>
        <rdfs:domain rdf:resource="&ProposedContract;Work"/>
        <rdfs:subPropertyOf rdf:resource="&ProposedContract;atMainPlace"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#hasObject -->

    <owl:ObjectProperty rdf:about="&ProposedContract;hasObject">
        <rdfs:label xml:lang="en">has object (1..*)</rdfs:label>
        <rdfs:comment xml:lang="en">This property connects the public procurement contract to its subject matter (Work, Supply, or Service) - i.e. connects the LOTED2:ProposedPublicProcurementContract with LOTED2:ObjectOfTheContract.

Since the object of the contract is one of the essential requirements of the contract, there are no contracts without the corresponding object of the contract (necessary condition).</rdfs:comment>
        <rdfs:range rdf:resource="&ProposedContract;ObjectOfContract"/>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedContract"/>
        <owl:inverseOf rdf:resource="&ProposedContract;isObjectOf"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#hasPlaceOfExecution -->

    <owl:ObjectProperty rdf:about="&ProposedContract;hasPlaceOfExecution">
        <rdfs:label xml:lang="en">has place of execution (1..*)</rdfs:label>
        <rdfs:comment xml:lang="en">The place of execution of the contract. 

This property is a SuperPropertyOf the property chain: 
hasObject o atMainPlace.

This means that the place of the delivery of the supply or of the performance of the service or of the execution of works, is the place of the execution of the contract.

In this way is possible to indicate, according to Good Relations ontology, the Location where the gr: Offering is available.</rdfs:comment>
        <rdfs:range rdf:resource="&ProcurementsTopClasses;Location"/>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedContract"/>
        <owl:inverseOf rdf:resource="&ProposedContract;isPlaceOfExecutionOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&ProposedContract;hasObject"/>
            <rdf:Description rdf:about="&ProposedContract;atMainPlace"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isDividedInto -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isDividedInto">
        <rdfs:label xml:lang="en">is divided into (1..*)</rdfs:label>
        <rdfs:comment xml:lang="en">The proposed contract (of type lot) in which is divided a proposed contract.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ProposedContract;ProposedPublicProcurementContract"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ProposedContract;typeOfProposal"/>
                        <owl:hasValue rdf:resource="&ProposedContract;ProposedContractDividedIntoLots"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ProposedContract;ProposedPublicProcurementContract"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ProposedContract;typeOfProposal"/>
                        <owl:hasValue rdf:resource="&ProposedContract;Lot"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isMainPlaceOf -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isMainPlaceOf">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">is main place of performance of contract (1..*)</rdfs:label>
        <owl:inverseOf rdf:resource="&ProposedContract;atMainPlace"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isMainPlaceOfDeliveryOf -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isMainPlaceOfDeliveryOf">
        <rdfs:label xml:lang="en">is main place of delivery of (1..*)</rdfs:label>
        <owl:inverseOf rdf:resource="&ProposedContract;hasMainPlaceOfDelivery"/>
        <rdfs:subPropertyOf rdf:resource="&ProposedContract;isMainPlaceOf"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isMainPlaceOfPerformanceOf -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isMainPlaceOfPerformanceOf">
        <rdfs:label xml:lang="en">is main place of performance of (1..*)</rdfs:label>
        <owl:inverseOf rdf:resource="&ProposedContract;hasMainPlaceOfPerformance"/>
        <rdfs:subPropertyOf rdf:resource="&ProposedContract;isMainPlaceOf"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isMainPlaceOfWorks -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isMainPlaceOfWorks">
        <rdfs:label xml:lang="en">is main place of works (1..*)</rdfs:label>
        <owl:inverseOf rdf:resource="&ProposedContract;hasMainPlaceOfWorks"/>
        <rdfs:subPropertyOf rdf:resource="&ProposedContract;isMainPlaceOf"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isObjectOf -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isObjectOf">
        <rdfs:label xml:lang="en">is object of (1..*)</rdfs:label>
        <rdfs:comment xml:lang="en">Note: an object of contract can be the same for several contracts, consider for an example standard form contracts or contracts of adhesion.</rdfs:comment>
        <rdfs:domain rdf:resource="&ProposedContract;ObjectOfContract"/>
        <rdfs:range rdf:resource="&ProposedContract;ProposedContract"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isPlaceOfExecutionOf -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isPlaceOfExecutionOf">
        <rdfs:label xml:lang="en">is place of execution of (1..*)</rdfs:label>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&ProposedContract;isMainPlaceOf"/>
            <rdf:Description rdf:about="&ProposedContract;isObjectOf"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#isTypeOfProposal -->

    <owl:ObjectProperty rdf:about="&ProposedContract;isTypeOfProposal">
        <rdfs:label xml:lang="en">is type of proposal (1..*)</rdfs:label>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedContractType"/>
        <rdfs:range rdf:resource="&ProposedContract;ProposedPublicProcurementContract"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#partOf -->

    <owl:ObjectProperty rdf:about="&ProposedContract;partOf">
        <rdfs:comment xml:lang="en">The proposed contract (of type lot) that is part of another proposed contract.</rdfs:comment>
        <rdfs:label xml:lang="en">part of (1..1)</rdfs:label>
        <owl:inverseOf rdf:resource="&ProposedContract;isDividedInto"/>
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ProposedContract;ProposedPublicProcurementContract"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ProposedContract;typeOfProposal"/>
                        <owl:hasValue rdf:resource="&ProposedContract;Lot"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ProposedContract;ProposedPublicProcurementContract"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ProposedContract;typeOfProposal"/>
                        <owl:hasValue rdf:resource="&ProposedContract;ProposedContractDividedIntoLots"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    
<!-- http://loted.eu/ontology2/core#throughWhichIsAnnouncedTenderForAward -->
	<owl:ObjectProperty rdf:about="&ProposedContract;throughWhichIsAnnouncedTenderForAward">
		<rdfs:label xml:lang="en">through which is announced tender for award (1..*)</rdfs:label>
		<rdfs:comment xml:lang="en">This property is a shortcut to express for the award of which contract is issued the notice.</rdfs:comment>
		<rdfs:range rdf:resource="&ProposedContract;ProposedPublicProcurementContract"/>
		<rdfs:domain rdf:resource="&TenderDocuments;TenderDocument"/>
		<owl:propertyChainAxiom rdf:parseType="Collection">
			<rdf:Description rdf:about="&TenderDocuments;throughWhichIsAnnounced"/>
			<rdf:Description rdf:about="&ProcurementCompetitiveProcess;forAwardOf"/>
		</owl:propertyChainAxiom>
	</owl:ObjectProperty>

<!-- http://loted.eu/ontology2/core#throughWhichIsAnnouncedTenderForAwardLot -->
	<owl:ObjectProperty rdf:about="&ProposedContract;throughWhichIsAnnouncedTenderForAwardLot">
		<rdfs:label xml:lang="en">through which is announced tender for award lot (0..*)</rdfs:label>
		<rdfs:domain rdf:resource="&TenderDocuments;TenderDocument"/>
		<owl:propertyChainAxiom rdf:parseType="Collection">
			<rdf:Description rdf:about="&ProposedContract;throughWhichIsAnnouncedTenderForAward"/>
			<rdf:Description rdf:about="&ProposedContract;isDividedInto"/>
		</owl:propertyChainAxiom>
		<rdfs:range>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&ProposedContract;ProposedPublicContract"/>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&ProposedContract;typeOfProposal"/>
						<owl:hasValue rdf:resource="&ProposedContract;Lot"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</rdfs:range>
	</owl:ObjectProperty>


    <!-- https://loted2.googlecode.com/svn/trunk/modules/ted#typeOfProposal -->

    <owl:ObjectProperty rdf:about="&ProposedContract;typeOfProposal">
        <rdfs:label xml:lang="en">type of proposal (1..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The property aims to link the contract with its type: contract divided into lots, contract not divided into lots or lot.</rdfs:comment>
        <rdfs:range rdf:resource="&ProposedContract;ProposedContractType"/>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedPublicProcurementContract"/>
        <owl:inverseOf rdf:resource="&ProposedContract;isTypeOfProposal"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeinterval#hasStartingDate -->

    <owl:DatatypeProperty rdf:about="&ProposedContract;hasStartingDate">
        <rdfs:label xml:lang="en">has starting date (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The starting date of the contract.</rdfs:comment>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedContract"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeinterval#hasTimeLimitForCompletion -->

    <owl:DatatypeProperty rdf:about="&ProposedContract;hasTimeLimitForCompletion">
        <rdfs:label xml:lang="en">has completion date (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The time limit for completion of the contract.</rdfs:comment>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedContract"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#admitsVariants -->

    <owl:DatatypeProperty rdf:about="&ProposedContract;admitsVariants">
        <rdfs:label xml:lang="en">admits variants (0..*)</rdfs:label>
        <rdfs:domain rdf:resource="&ProposedContract;ProposedContract"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#description -->

    <owl:DatatypeProperty rdf:about="&ProposedContract;description">
        <rdfs:label xml:lang="en">description (0..1)</rdfs:label>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/contract#durationOfContractInDays -->

    <owl:DatatypeProperty rdf:about="&ProposedContract;durationOfContractInDays">
        <rdfs:label xml:lang="en">duration of the contract in months (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The duration of contract in days (from the date of the award)</rdfs:comment>
