    <!-- https://loted2.googlecode.com/svn/trunk/modules/ProcurementSubjectiveScope#AuthoritiesAssociation -->

    <owl:Class rdf:about="&modules;ProcurementSubjectiveScope#AuthoritiesAssociation">
        <rdfs:label xml:lang="en">Authorities association</rdfs:label>
        <rdfs:subClassOf rdf:resource="&modules;ProcurementsTopClasses#LegalPerson"/>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#BodiesGovernedByPublicLawAssociation"/>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#BodyGovernedByPublicLaw"/>
