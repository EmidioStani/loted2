        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#AuthoritiesAssociation"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#BodiesGovernedByPublicLawAssociation"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#BodyGovernedByPublicLaw"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#CentralPurchasingBody"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#EuropeanInstitutionOrInternationalOrganisation"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#PublicAuthority"/>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&modules;ProcurementSubjectiveScope#hasMainActivity"/>
                        <owl:someValuesFrom rdf:resource="&modules;ProcurementSubjectiveScope#OrdinarySectorActivity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&modules;ProcurementsTopClasses#LegalPerson"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#AuthoritiesAssociation"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#BodiesGovernedByPublicLawAssociation"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#BodyGovernedByPublicLaw"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#CentralPurchasingBody"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#EuropeanInstitutionOrInternationalOrganisation"/>
                            <rdf:Description rdf:about="&modules;ProcurementSubjectiveScope#PublicAuthority"/>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&modules;ProcurementSubjectiveScope#hasMainActivity"/>
                        <owl:allValuesFrom rdf:resource="&modules;ProcurementSubjectiveScope#OrdinarySectorActivity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#EntityOperatingInUtilities"/>
        <rdfs:comment xml:lang="en">This is a class used to classify entities on the basis of the main activities they carry out.

From this classification depends the application of Directive 2004/18/EC instead of Directive 2004/17/EC.</rdfs:comment>
    </owl:Class>
    


