    <!-- https://loted2.googlecode.com/svn/trunk/modules/ProcurementSubjectiveScope#BodiesGovernedByPublicLawAssociation -->

    <owl:Class rdf:about="&modules;ProcurementSubjectiveScope#BodiesGovernedByPublicLawAssociation">
        <rdfs:label xml:lang="en">Bodies governed by public law association</rdfs:label>
        <rdfs:subClassOf rdf:resource="&modules;ProcurementsTopClasses#LegalPerson"/>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#CentralPurchasingBody"/>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#EntityOperatingOnTheBasisOfSpecialOrExclusiveRights"/>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#EuropeanInstitutionOrInternationalOrganisation"/>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#PublicAuthority"/>
        <owl:disjointWith rdf:resource="&modules;ProcurementSubjectiveScope#PublicUndertaking"/>
