    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&TenderDocuments;tedLabel"/>
    <owl:AnnotationProperty rdf:about="&TenderDocuments;tedDataID"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
 <!-- https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#covers -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;covers">
        <rdfs:label xml:lang="en">covers (0..*)</rdfs:label>
        <rdfs:comment xml:lang="en">This property is intended to describe the LOTED2:ProcurementLegalSource that covers a LOTED2:TenderDocument. 

For example a LOTED2:ContractNotice is covered by the Directive 2004/18/EC if it is announced by a LOTED2:EntityOperatingInOrdinarySector. On the other hand, if it is announced by a LOTED2:EntityOperatingInUtilities it is covered by the Directive 2004/17/EC.

In addition, a contract notice can be covered also by the Government Procurement Agreement. This happens when the public contract to be awarded has the features of the contracts that are covered by this Agreement.</rdfs:comment>
        <rdfs:domain rdf:resource="&ProcurementRegulations;PublicProcurementLegalSource"/>
        <rdfs:range rdf:resource="&TenderDocuments;TenderDocument"/>
    </owl:ObjectProperty>
    


    <!-- https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#isCoveredBy -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;isCoveredBy">
        <rdfs:label xml:lang="en">is covered by (1..*)</rdfs:label>
        <rdfs:domain rdf:resource="&TenderDocuments;TenderDocument"/>
        <rdfs:range rdf:resource="&ProcurementRegulations;PublicProcurementLegalSource"/>
        <owl:inverseOf rdf:resource="&TenderDocuments;covers"/>
    </owl:ObjectProperty>
    

    <!-- &TenderDocuments;hasAdditionalDocument -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;hasAdditionalDocument">
        <rdfs:label xml:lang="en">has additional document (0..*)</rdfs:label>
        <rdfs:range rdf:resource="&TenderDocuments;AdditionalTenderDocument"/>
        <rdfs:subPropertyOf rdf:resource="&TenderDocuments;hasAttachedDocument"/>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;hasAttachedDocument -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;hasAttachedDocument">
        <rdfs:label xml:lang="en">has attached document (0..*)</rdfs:label>
        <rdfs:comment xml:lang="en">In the LOTED2 ontology this property does not refer to attached documents as understood in a common sense. A specifications document or an invitation letter to submit a tender (in case of restrictive procedure) attached to a: 

     LOTED2:ContractNotice (public sectors and utilities)
 or LOTED2:WorksAwardedByTheConcessionaire
 or LOTED2:PublicWorksConcessionNotice
 or LOTED2:DesignContestNotice
 or LOTED2:QualificationSystemWithCallForCompetition
 or LOTED2:QualificationSystemWithoutCallForCompetition
 or LOTED2:PeriodicIndicativeNoticeWithCallForCompetition
 or LOTED2:SimplifiedContractNoticeOnADynamicPurchasingSystem
 or LOTED2:WorksContractAwardedByTheConcessionaireNotice

are not simple &#39;attached documents&#39;, because they define together with the document to which are attached (for example the contract notice) the &#39;lex specialis&#39; of the tender (i.e. the set of rules governing the tender or the design contest or the qualification system).</rdfs:comment>
        <rdfs:range rdf:resource="&TenderDocuments;AttachedTenderDocument"/>
        <owl:inverseOf rdf:resource="&TenderDocuments;isAttachedDocumentOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&TenderDocuments;ContractNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;DesignContestNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;PeriodicIndicativeNoticeWithCallForCompetition"/>
                    <rdf:Description rdf:about="&TenderDocuments;PublicWorkConcessionNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;QualificationSystemWithCallForCompetitionNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;QualificationSystemWithoutCallForCompetitionNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;SimplifiedContractNoticeOnADynamicPurchasingSystem"/>
                    <rdf:Description rdf:about="&TenderDocuments;WorksContractAwardedByTheConcessionaireNotice"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;hasSpecificationsDocument -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;hasSpecificationsDocument">
        <rdfs:label xml:lang="en">has specifications document (0..1)</rdfs:label>
        <rdfs:range rdf:resource="&TenderDocuments;SpecificationsDocument"/>
        <rdfs:subPropertyOf rdf:resource="&TenderDocuments;hasAttachedDocument"/>
    </owl:ObjectProperty>
    
<!-- &TenderDocuments;hasSought -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;hasSought">
		<rdfs:label xml:lang="en">has sought (0..*)</rdfs:label>
		<rdfs:comment xml:lang="en">This property can be used to indicate that a natural or legal person has sought an invitation letter to submit a tender</rdfs:comment>
		<rdfs:domain>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&ProcurementsTopClasses;LegalPerson"/>
					<rdf:Description rdf:about="&ProcurementsTopClasses;NaturalPerson"/>
				</owl:unionOf>
			</owl:Class>
		</rdfs:domain>
                <rdfs:range rdf:resource="&TenderDocuments;InvitationToSubmitATender"/>
	</owl:ObjectProperty>

    <!-- &TenderDocuments;isAdditionalDocumentOf -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;isAdditionalDocumentOf">
        <rdfs:label xml:lang="en">is additional document of (1..1)</rdfs:label>
        <rdfs:domain rdf:resource="&TenderDocuments;AdditionalTenderDocument"/>
        <rdfs:subPropertyOf rdf:resource="&TenderDocuments;isAttachedDocumentOf"/>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;isAttachedDocumentOf -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;isAttachedDocumentOf">
        <rdfs:label xml:lang="en">is attached document of (1..1)</rdfs:label>
        <rdfs:domain rdf:resource="&TenderDocuments;AttachedTenderDocument"/>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;isFollowingPublicationOf -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;isFollowingPublicationOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">is following publication of (0..*)</rdfs:label>
        <rdfs:domain rdf:resource="&TenderDocuments;TenderDocument"/>
        <rdfs:range rdf:resource="&TenderDocuments;TenderDocument"/>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;isPreviousPublicationOf -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;isPreviousPublicationOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">is previous publication of (0..*)</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a LOTED2:TenderDocument to the its previous LOTED2:TenderDocument, both concerning the same tender and/or contract.

This property is relevant for connecting: 

- a LOTED2:PriorInformationNotice or a LOTED2:NoticeOnBuyerProfile // to a LOTED2: ContractNotice (following publication); 

- a LOTED2:PeriodicIndicativeNotice or a LOTED2:NoticeOnBuyerProfile // to a LOTED2: ContractNoticeInUtilitiesSectors (following publication);

- a LOTED2:PriorInformationNotice or a LOTED2:NoticeOnBuyerProfile or a LOTED2: ContractNotice or a LOTED2: SimplifiedContractNoticeOnADynamicPurchasingSystem or a LOTED2: VoluntaryExAnteTransparencyNotice // to a LOTED2: ContractAwardNotice (following publication);

- a LOTED2:PeriodicIndicativeNotice or a LOTED2: NoticeOnExistenceOfQualificationSystem or a LOTED2: NoticeOnBuyerProfile or a LOTED2: Contractnotice or a LOTED2: SimplifiedContractNoticeOnADynamicPurchasingSystem or a LOTED2: VoluntaryExAnteTransparencyNotice // to a LOTED2: ContractAwardNotice-Utilities (following publication);

- a LOTED2:PeriodicIndicativeNotice or a LOTED2: NoticeOnExistenceOfQualificationSystem or a LOTED2: PriorInformationNotice or a LOTED2: NoticeOnBuyerProfile or LOTED2: ContractNotice or a LOTED2: SimplifiedContractNoticeOnADynamicPurchasingSystem // to a LOTED2: VoluntaryExAnteTransparencyNotice (following publication).
</rdfs:comment>
        <owl:inverseOf rdf:resource="&TenderDocuments;isFollowingPublicationOf"/>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;isSpecificationsDocumentOf -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;isSpecificationsDocumentOf">
        <rdfs:label xml:lang="en">is specifications document of (1..1)</rdfs:label>
        <rdfs:domain rdf:resource="&TenderDocuments;SpecificationsDocument"/>
        <rdfs:subPropertyOf rdf:resource="&TenderDocuments;isAttachedDocumentOf"/>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;publishedIn -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;publishedIn">
        <rdfs:label xml:lang="en">published in (0..1)</rdfs:label>
        <rdfs:range rdf:resource="&TenderDocuments;OJEU-SeriesS"/>
        <rdfs:domain rdf:resource="&TenderDocuments;TenderDocument"/>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;relatesToThePublicationOf -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;relatesToThePublicationOf">
        <rdfs:label xml:lang="en">relates to the publication of</rdfs:label>
        <rdfs:comment xml:lang="en">The Contracting authorities which publish a prior information notice on their buyer profiles shall send the Commission, electronically, a notice of the publication of the prior information notice on a buyer profile (art. 35 Directive 2004/18/EC). 

The same is stated for Contracting entities which publish a periodic indicative notice on their buyer profiles. They shall transmit to the Commission, electronically, a notice of the publication of the periodic indicative notice on a buyer profile (art. 41 Directive 2004/17/EC). 

So any notice on buyer profile relates to the publication of a prior information notice or a peridoc indicative notice without call for competition.</rdfs:comment>
        <rdfs:domain rdf:resource="&TenderDocuments;NoticeOnBuyerProfile"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&TenderDocuments;PeriodicIndicativeNoticeWithoutCallForCompetition"/>
                    <rdf:Description rdf:about="&TenderDocuments;PriorInformationNotice"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;isAnnouncedThrough -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;isAnnouncedThrough">
        <rdfs:label xml:lang="en">is announced through (1..1)</rdfs:label>
    </owl:ObjectProperty>
    


    
    


    <!-- &TenderDocuments;throughWhichIsAnnounced -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;throughWhichIsAnnounced">
        <rdfs:label xml:lang="en">through which is announced (1..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The function of a contract notice or of other types of notices published in the OJEU Series S is to announce a procurement competitive process, that can be a tender (i.e. a competitive bidding), a design contest or a qualification system. 

Through a contract notice is announced a tender; through a design contest notice is announced a design contest, and through a qualification system notice is announced a qualification system. 

A qualification system notice with call for competition announces also a tender.</rdfs:comment>
        <owl:inverseOf rdf:resource="&TenderDocuments;isAnnouncedThrough"/>
        <rdfs:range rdf:resource="&ProcurementCompetitiveProcess;ProcurementCompetitiveProcess"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&TenderDocuments;ContractNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;DesignContestNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;PeriodicIndicativeNoticeWithCallForCompetition"/>
                    <rdf:Description rdf:about="&TenderDocuments;PublicWorkConcessionNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;QualificationSystemWithCallForCompetitionNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;QualificationSystemWithoutCallForCompetitionNotice"/>
                    <rdf:Description rdf:about="&TenderDocuments;SimplifiedContractNoticeOnADynamicPurchasingSystem"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- &TenderDocuments;throughWhichIsAnnouncedForthcoming -->

    <owl:ObjectProperty rdf:about="&TenderDocuments;throughWhichIsAnnouncedForthcoming">
        <rdfs:label xml:lang="en">through which is announced forthcoming (1..*)</rdfs:label>
        <rdfs:comment xml:lang="en">With a: 

- periodic indicative notice without call for competition (issued by only contracting entities)

or a

- prior information notice (issued by only contracting authorities)

are pre-announced forthcoming tenders to be held in the following year by the entity. 

The function of this notice is to alert the market for the upcoming opportunities, i.e. the offerings that awarding authorities will seek.</rdfs:comment>
        <rdfs:range rdf:resource="&ProcurementCompetitiveProcess;Tender"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&TenderDocuments;PeriodicIndicativeNoticeWithoutCallForCompetition"/>
                    <rdf:Description rdf:about="&TenderDocuments;PriorInformationNotice"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


