     xml:base="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments"
     xmlns:Directive2004="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#Directive2004/17/"
     xmlns:Directive2009="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#Directive2009/81/"
     xmlns:ProcurementCompetitiveProcess="https://loted2.googlecode.com/svn/trunk/modules/TenderDocumentsProcurementCompetitiveProcess#"
     xmlns:Directive2007="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#Directive2007/66/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ProcurementsTopClasses="https://loted2.googlecode.com/svn/trunk/modules/ProcurementsTopClasses#"
     xmlns:ProcurementSubjectiveScope="https://loted2.googlecode.com/svn/trunk/modules/ProcurementSubjectiveScope#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:Directive20042="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#Directive2004/18/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:TenderDocuments="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#"
     xmlns:EEA="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments#B-EuropeanEconomicArea(EEA)"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ProcurementRegulations="https://loted2.googlecode.com/svn/trunk/modules/ProcurementRegulations#"
>
    <owl:Ontology rdf:about="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments">
        <rdfs:label xml:lang="en">Tender Documents</rdfs:label>
        <owl:versionInfo xml:lang="en">2.0</owl:versionInfo>
        <rdfs:comment xml:lang="en">The Tender Documents module, which is part of the LOTED2 ontology, aims at representing the set of public procurement notices (contract notices, design contest notices, prior information notices, etc.) which are published on the Tenders Electronic Daily (TED) system (http://ted.europa.eu/).</rdfs:comment>
        <owl:versionIRI rdf:resource="https://loted2.googlecode.com/svn/trunk/modules/TenderDocuments"/>
        <dc:title rdf:datatype="&xsd;string">LOTED2: TenderDocuments Module</dc:title>
        <dc:contributor rdf:datatype="&xsd;string">Enrico Motta</dc:contributor>
         <dc:contributor rdf:datatype="&xsd;string">Mathieu d'Aquin</dc:contributor>
		<dc:description rdf:datatype="&xsd;string">The ontology was created by Isabella Distinto, with input from Mathieu d'Aquin and Enrico Motta.</dc:description>
        <dc:creator rdf:datatype="&xsd;string">Isabella Distinto</dc:creator>
        <dc:language rdf:datatype="&xsd;language">en</dc:language>
        <dc:date>2013-07-24</dc:date>
        <owl:imports rdf:resource="https://loted2.googlecode.com/svn/trunk/modules/ProcurementCompetitiveProcess.owl"/>
        <owl:imports rdf:resource="https://loted2.googlecode.com/svn/trunk/modules/ProcurementRegulations.owl"/>

    </owl:Ontology>
    


